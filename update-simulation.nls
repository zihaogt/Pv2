    let randnum random-float 1
    let found? false
    let indexval 6
    while  [not found?]
    [ if randnum >= (item (CD4-stratum - 1) (item indexval  OI-probabilities) )
      [set found? true
       set OI-type indexval + 1
      ]
     set indexval indexval - 1
    ]
    if OI-type = 7
    [set OI-type 0]
   
