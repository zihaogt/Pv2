    [set stage 1
    ]
  ;' non acute
    [Ifelse aware? = false;' not aware yet
      [set stage 2] ;' sexual or IDU, unaware, non on haart, not acute
      [ ;aware   
        if on-treatment? = true
        [ifelse regimen = 40 or regimen = 10
          [set stage 5]
          [set stage 6]
        ]
       
        
